{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": "off",
        "consistent-return": "off",
        "keyword-spacing": "off",
        "function-paren-newline": "off",
        "implicit-arrow-linebreak":"off",
        "radix":"off",
        "arrow-parens": "off",
        "no-extra-semi": "off",
        "no-else-return": "off",
        "comma-dangle": "off",
        "import/no-extraneous-dependencies": "off",
        "no-unused-vars": "off",
        "object-curly-newline": "off",
        "max-len": "off",
        "no-trailing-spaces":"off",
        "indent": "off",
        "eol-last": "off"
    }
}
